{
  "include": [
    "$(MODDABLE)/examples/manifest_base.json",
    "$(MODDABLE)/examples/manifest_net.json",
    "$(MODDABLE)/examples/io/tcp/fetch/manifest_fetch.json",
    "$(MODDABLE)/examples/pins/audioout/elevenlabs-stream/manifest_elevenlabsstreamer.json"
  ],
  "modules": {
    "*": ["./main", "$(MODULES)/pins/audioin/*"],
    "pins/*": ["$(MODULES)/pins/i2s/*"]
  },
  "defines": {
    "audioIn": {
      "sampleRate": 16000,
      "bitsPerSample": 16
    },
    "audioOut": {
      "bitsPerSample": 16,
      "numChannels": 1,
      "sampleRate": 44100,
      "queueLength": 12
    }
  },
  "data": {
    "*": ["$(MODULES)/crypt/data/ca233"]
  },
  "creation": {
    "static": 442368,
    "chunk": {
      "initial": 8192,
      "incremental": 2048
    },
    "heap": {
      "initial": 512,
      "incremental": 64
    }
  },
  "config": {
    "google_api_key": "YOUR-API-KEY",
    "elevenlabs_api_key": "YOUR-API-KE"
  },
  "preload": ["audioin"],
  "platforms": {
    "esp32": {
      "modules": {
        "*": "$(MODULES)/pins/audioin/esp32/*"
      }
    },
    "...": {
      "error": "audioIn module unsupported"
    }
  }
}
